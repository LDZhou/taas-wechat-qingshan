/* cooperative brand */

.cooperative-brand{
  padding: 160rpx 80rpx 194rpx 88rpx;
  background-color: #19181d;
  position: relative;
}

.cooperation-bg{
  position: absolute;
  bottom: 0;
  right: 42rpx;
  width: 448rpx;
  height: 634rpx;
  z-index: 10;
}

.cooperation-title{
  font-size: 40rpx;
  margin-bottom: 36rpx;
  text-align: center;
}

.turn-bottom-icon-wrap{
  text-align: center;
}

.turn-bottom-icon-wrap > image{
  width: 68rpx;
  height: 68rpx;
  margin-top: 10rpx;
}

.cooperation-content-container{
  margin-top: 56rpx;
}

.cooperation-content-wrap{
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.cooperation-content-wrap ~ .cooperation-content-wrap{
  margin-top: 100rpx;
}

.cooperation-content-wrap > text{
  width: 374rpx;
  font-size: 32rpx;
  line-height: 52rpx;
  z-index: 100;
}

.cooperation-icon{
  width: 160rpx;
  height: 160rpx;
}

